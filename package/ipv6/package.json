{
  "name": "@deplug/ipv6",
  "version": "0.1.0",
  "license": "MIT",
  "description": "IPv6 dissector",
  "engines": {
    "deplug": "*"
  },
  "deplug": {
    "components": [
      {
        "type": "core:dissector:packet",
        "main": "ipv6"
      },
      {
        "type": "core:filter:macro",
        "id": "ipv6-literal",
        "name": "IPv6 Address",
        "description": "{{prefix}}::1",
        "main": "macro.js"
      },
      {
        "type": "core:renderer:attr",
        "id": "@ipv6:addr",
        "main": "ipv6-addr.js",
        "macro": "macro-rev.js"
      },
      {
        "type": "core:style",
        "files": [
          "ipv6.less"
        ]
      },
      {
        "type": "core:token",
        "files": [
          "tokens.json"
        ]
      }
    ]
  }
}
