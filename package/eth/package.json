{
  "name": "@deplug/eth",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Ethernet dissector",
  "engines": {
    "deplug": "*"
  },
  "deplug": {
    "components": [
      {
        "type": "core:dissector:packet",
        "main": "main.node",
        "linkLayers": [
          {
            "link": 1,
            "id": "[eth]"
          }
        ],
        "samples": [
          {
            "pcap": "tcp-ipv4.pcap",
            "assert": "assert.json"
          }
        ]
      },
      {
        "type": "core:renderer:attr",
        "id": "@eth:mac",
        "main": "mac-addr.js"
      },
      {
        "type": "core:style",
        "files": [
          "eth.less"
        ]
      },
      {
        "type": "core:token",
        "files": [
          "tokens.json"
        ]
      }
    ]
  }
}
