{
  "name": "@deplug/pcap-file",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Pcap File Importer / Exporter",
  "engines": {
    "deplug": "*"
  },
  "deplug": {
    "components": [
      {
        "type": "core:file:importer",
        "main": "importer.node",
        "extensions": {
          "pcap": "Pcap File"
        }
      },
      {
        "type": "core:file:exporter",
        "main": "exporter.node",
        "extensions": {
          "pcap": "Pcap File"
        }
      }
    ]
  }
}
