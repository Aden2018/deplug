{
  "name": "@deplug/tcp",
  "version": "0.1.0",
  "license": "MIT",
  "description": "TCP dissector",
  "engines": {
    "deplug": "*"
  },
  "deplug": {
    "components": [
      {
        "type": "core:dissector:packet",
        "main": "tcp"
      },
      {
        "type": "core:dissector:stream",
        "main": "tcp-stream.node"
      },
      {
        "type": "core:renderer:layer",
        "id": "tcp",
        "main": "tcp-summary.js"
      },
      {
        "type": "core:style",
        "files": [
          "tcp.less"
        ]
      },
      {
        "type": "core:token",
        "files": [
          "tokens.json"
        ]
      }
    ]
  }
}
