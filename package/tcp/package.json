{
  "name": "@deplug/tcp",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Deplug TCP dissector",
  "engines": {
    "deplug": "*"
  },
  "deplug": {
    "components": [
      {
        "type": "core:dissector:packet",
        "main": "tcp.node"
      },
      {
        "type": "core:dissector:stream",
        "main": "tcp-stream.node"
      },
      {
        "type": "core:renderer:layer",
        "id": "tcp",
        "main": "tcp-summary.js"
      }
    ]
  }
}
